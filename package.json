{
	"name": "osmnl-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run generate && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate": "npm run generate:blog && npm run generate:projects",
		"generate:blog": "vite-node ./scripts/generatePosts.ts -- blog blog src/routes/blog/posts.json",
		"generate:projects": "vite-node ./scripts/generatePosts.ts -- projects project src/routes/projects/projects.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.17",
		"@types/node": "^22.0.0",
		"@typescript-eslint/eslint-plugin": "^7.12.0",
		"@typescript-eslint/parser": "^7.13.1",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.41.0",
		"postcss": "^8.4.38",
		"prettier": "^3.3.1",
		"prettier-plugin-svelte": "^3.2.5",
		"svelte": "^4.2.17",
		"svelte-check": "^3.8.0",
		"svelte-preprocess": "^5.1.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"vite": "^5.3.1"
	},
	"type": "module",
	"dependencies": {
		"front-matter": "^4.0.2",
		"osm-community-index": "^5.7.1",
		"svelte-exmarkdown": "^3.0.5",
		"sveltekit-i18n": "^2.4.2",
		"vite-node": "^1.6.0"
	}
}
